{
  "name": "tips.js",
  "title": "tips.js",
  "version": "1.1.5",
  "description": "jQuery plugin for writing dynamic tooltips",
  "main": "dist/tips.js",
  "homepage": "https://github.com/skyout/tips.js",
  "author": "Scott Lanning <skyout@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/skyout/tips.js.git"
  },
  "bugs": {
    "url": "https://github.com/skyout/tips.js/issues/"
  },
  "dependencies": {
    "jquery": ">=1.7.0"
  },
  "devDependencies": {
    "coffee-script": "^1.10.0",
    "stylus": "^0.52.4"
  },
  "scripts": {
    "compile:coffee": "coffee --bare --output dist/js/ --compile src/coffee/",
    "compile:stylus": "stylus src/stylus/tips.styl --out dist/css/",
    "watch:coffee": "coffee --bare --watch --output dist/js/ --compile src/coffee/",
    "watch:stylus": "stylus --watch src/stylus/tips.styl --out dist/css/",
    "watch:demo": "stylus --watch demo/stylus/styles.styl --out demo/css/",
    "demo:coffee": "coffee --bare --output demo/js/ --compile src/coffee/",
    "demo:stylus": "stylus --compress demo/stylus/styles.styl --out demo/css/",
    "build": "npm run compile:coffee && npm run compile:stylus",
    "build:demo": "npm run demo:coffee && npm run demo:stylus"
  },
  "keywords": [
    "tooltips",
    "jquery",
    "tips",
    "plugin"
  ],
  "license": "MIT"
}
