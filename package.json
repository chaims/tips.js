{
    "name": "tips.js",
    "title": "tips.js",
    "version": "1.1.3",
    "description": "jQuery plugin for writing dynamic tooltips",
    "main": "dist/tips.js",
    "homepage": "https://github.com/skyout/tips.js",
    "author":
    {
        "name": "Scott Lanning",
        "email": "skyout@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:skyout/tips.js.git"
    },
    "bugs": {
        "url": "https://github.com/skyout/tips.js/issues/"
    },
    "dependencies": {
        "jquery": ">=1.7.0"
    },
    "devDependencies": {
        "coffee-script": "^1.10.0",
        "stylus": "^0.52.4"
    },
   "scripts": {
        "compile-coffee": "coffee --bare --output dist/ --compile src/",
        "compile-stylus": "stylus stylus/tips.styl --out css/",
        "watch-coffee": "coffee --bare --watch --output dist/ --compile src/",
        "watch-stylus": "stylus --watch stylus/tips.styl --out css/"
    },
    "keywords": [
        "tooltips",
        "jquery",
        "tips",
        "plugin"
    ],
    "license": "MIT"
}
